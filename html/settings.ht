<!-- BEGIN settings -->
<form method="post" action="{URI_URL}#list" name="frm">
	<table>
		<tbody>
			<tr>
				<td>
					Наименование агенства: 
				</td>
				<td>
					<input type="text" name="agency_name" class="settings" value="{AGENCY_NAME}"> 
				</td>
			</tr>
			<tr>
				<td>
					Город: 
				</td>
				<td>
					<input type="text" name="agency_name" class="settings" value="{OFFICE_CITY}"> 
				</td>
			</tr>
			<tr>
				<td>
					Номер офиса: 
				</td>
				<td>
